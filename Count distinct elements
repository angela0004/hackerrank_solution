import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        
        int n = sc.nextInt();
        int[] arr = new int[n];

        
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }

        
        Set<Integer> distinct = new HashSet<>();
        for (int num : arr) {
            distinct.add(num);
        }

        
        System.out.println("There are " + distinct.size() + " distinct element in the array.");
    }
}
